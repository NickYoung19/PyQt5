QLabel#notice {
    font-size: 20px;
    color: red;
    border: 1px solid gray;
    border-radius: 8px;
}
#notice1 {
    color: blue;
}

QLabel#notice[notice_level="normal"] {
    color: green;
    border-color: green;
}
QLabel#notice[notice_level="warning"] {
    color: #f38b00;
    border-color: #f38b00;
}
QLabel#notice[notice_level="error"] {
    color: red;
    border-color: red;
}
